{
    "name": "randa_back_stopper",
    "version": "0.0.1",
    "description": "BackstopJS を使用したテスト用プロジェクトです。",
    "main": "bin/tasks/test.js",
    "scripts": {
        "start": "npm-run-all -s backstop:init backstop:reference",
        "restart": "npm-run-all -s backstop:init backstop:reference backstop:test",
        "test": "npm-run-all -s backstop:init backstop:reference backstop:test",
        "backstop:init": "node bin/tasks/init",
        "backstop:reference": "node bin/tasks/reference",
        "backstop:test": "node bin/tasks/test"
    },
    "author": "Arm Band",
    "license": "ISC",
    "devDependencies": {
        "backstopjs": "https://github.com/garris/BackstopJS#beta",
        "npm-run-all": "^4.1.5",
        "yaml": "^1.10.0"
    },
    "resolutions": {
        "puppeteer": "^5.5.0"
    }
}
